{
  "name": "IFC Server DevContainer",
  "dockerComposeFile": "docker-compose.yml",
  "service": "devcontainer",
  "workspaceFolder": "/workspaces/poolarserver-bim-template",
  "postCreateCommand": "bash /workspaces/poolarserver-bim-template/post_create.sh",
  "postStartCommand": "bash /workspaces/poolarserver-bim-template/.devcontainer/post_start.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csharp",
        "ms-python.python",
        "ms-azuretools.vscode-docker",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "remoteUser": "root"
}
